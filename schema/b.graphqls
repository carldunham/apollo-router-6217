# B

extend schema
  @link(
    url: "https://specs.apollo.dev/federation/v2.2"
    import: ["@key", "@external", "@shareable", "@composeDirective"]
  )

type Query {
  watchFeed: [Element!]
}

interface Element {
  post: Post
}

type CompactPostCommunityRecommendation implements Element {
  post: Post
}

type CardPostCommunityRecommendation implements Element {
  post: Post
}

interface Post {
  id: ID!
}

type SubredditPost implements Post @key(fields: "id") {
  id: ID!
}
