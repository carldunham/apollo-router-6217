# B

extend schema
  @link(
    url: "https://specs.apollo.dev/federation/v2.2"
    import: ["@key", "@external", "@shareable", "@composeDirective"]
  )

type Query {
  posts: [Element!]
}

interface Element {
  post: Post
}

type Compact implements Element {
  post: Post
}

type Full implements Element {
  post: Post
}

interface Post {
  id: ID!
}

type SimplePost implements Post @key(fields: "id") {
  id: ID!
}
